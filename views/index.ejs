<%- include('./partials/header') %>

    <main>

      <section class="banner1"></section>
      <h1 id="estilo">Encontre Seu Estilo</h1>
      <section class="banner-imgs">
        <div class="bota-preta"></div>
        <div class="bota-branca"></div>
      </section>

      <section class="imagens">
        <h2 id="desconto">30% OFF</h2>

        <div class="imagens-sapatos">

          <% for(let shoes of shoesList) { %>

          <div class="imagensP">
            <div class="sapato1">
              <img src="<%=shoes.picture%>">
            </div>
            <p><%= shoes.description %></p>
            <p> <%= shoes.price%></p>
          </div>

          <% } %>
      </section>

      <section class="banner-vestido">
        <div class="vestido1">
          <div class="descricao-sapatos">
            <h3>SANDÁLIA PALMILHA MATELASSÊ TIRAS ROLOTÊ</h3>
          </div>
          <h3 id="detalhes">VER DETALHES</h3>

          <div class="meio-corpo"></div>
        </div>

        <div class="vestido2"></div>
      </section>
    </main>
    <%- include('./partials/footer') %>
  </body>
</html>